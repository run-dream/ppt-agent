# 阿里云百炼 (DashScope) 配置
LLM_API_KEY=your_api_key_here
LLM_API_BASE=https://dashscope.aliyuncs.com/compatible-mode/v1

# 通义万相图像生成配置
IMAGE_GEN_MODEL=wanx-v1

# 图片搜索配置
# 搜索引擎开关 (true=启用搜索引擎, false=仅使用AI生成图片)
ENABLE_IMAGE_SEARCH_ENGINES=false

# 图片搜索配置 (免费选项，仅在ENABLE_IMAGE_SEARCH_ENGINES=true时使用)
# UNSPLASH_ACCESS_KEY=your_unsplash_access_key_here  # 注册获取：https://unsplash.com/developers
# UNSPLASH_SECRET_KEY=your_unsplash_secret_key_here  # 可选，用于 OAuth 应用

# 图片搜索配置 (付费选项，仅在ENABLE_IMAGE_SEARCH_ENGINES=true时使用)
# BING_SEARCH_API_KEY=your_bing_api_key_here  # 可选，每月 1000 次免费调用

# 模型配置 (针对不同节点)
# 大纲生成节点模型 (推荐使用能力最强的模型)
PLANNER_MODEL=qwen-max
# 内容生成节点模型
GENERATOR_MODEL=qwen-plus
# 配图建议节点模型
IMAGE_ADVISOR_MODEL=qwen-turbo

# 搜索引擎配置
BING_SEARCH_API_KEY=your_bing_api_key_here

# LangSmith 监控配置 (可选)
# LANGSMITH_API_KEY=your_langsmith_api_key_here    # LangSmith API Key
# LANGSMITH_PROJECT=chatppt-monitoring               # 项目名称
# LANGSMITH_ENDPOINT=https://api.smith.langchain.com # LangSmith 端点

# 其他配置
# WHISPER_MODEL=whisper-1
